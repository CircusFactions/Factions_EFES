<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.stoa.org/epidoc/schema/latest/tei-epidoc.rng" schematypens="http://relaxng.org/ns/structure/1.0"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Authority list of divine names and epithets in Factions</title>
            </titleStmt>
            <publicationStmt><ab/></publicationStmt>
            <sourceDesc><ab/></sourceDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            <div type="commentary">
                <listPerson type="divinities">
                    
                    <person xml:id="theos">
                        <persName xml:lang="en">God</persName>
                        <persName xml:lang="grc">Θεός</persName>
                        <persName xml:lang="grc">Κύριος</persName>
                    </person>
                    <person xml:id="maria">
                        <persName xml:lang="grc">Μαρία</persName>
                        <persName xml:lang="grc">Θεοτόκος</persName>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q345</idno>
                    </person>
                    <person xml:id="iesous">
                        <persName xml:lang="grc">Ἰησοῦς Χριστός</persName>
                        <persName xml:lang="en">Jesus Christ</persName>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q302</idno>
                    </person>
                    <person sameAs="#theos">
                        <persName xml:lang="en">Kyrios</persName>
                        <persName xml:lang="grc">Κύριος</persName>
                    </person>
                    <person xml:id="germanus">
                        <persName xml:lang="grc">Ἅγιος Γερμανός</persName>
                        <persName xml:lang="en">St Germanus</persName>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q302</idno>
                    </person>
                    <person xml:id="paulos">
                        <persName xml:lang="grc">Ἅγιος Παῦλος</persName>
                        <persName xml:lang="en">St Paul</persName>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q302</idno>
                    </person>
                    
                    
                    
                </listPerson>
            </div>
        </body>
    </text>
</TEI>